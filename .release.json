{
    "pkgFiles": ["package.json"],
    "buildCommand": "gulp build",
    "changelogCommand": "git log --pretty=format:'* %s [View Commit](https://code.ipreo.com/Ipreo/cupcake/commit/%H)'[REV_RANGE]",
    "dist": {
        "baseDir": null
    },
    "github": {
        "release": true,
        "releaseName": "Release %s",
        "preRelease": false,
        "draft": false,
        "tokenRef": "GITHUB_TOKEN"
    }    
}