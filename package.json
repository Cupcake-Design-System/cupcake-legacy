{
  "name": "@ipreo/cupcake",
  "version": "1.1.7",
  "description": "Ipreo CSS framework.",
  "main": "dist/default.css",
  "repository": {
    "type": "git",
    "url": "https://code.ipreo.com/Ipreo/cupcake"
  },
  "scripts": {
    "start": "gulp",
    "deploy:dist": "surge --project ./dist --domain ipreo-cupcake.surge.sh",
    "deploy:tests": "surge --project ./test --domain ipreo-cupcake-test.surge.sh",
    "deploy:surge": "npm run deploy:dist && npm run deploy:tests",
    "precommit": "lint-staged",
    "build": "gulp build",
    "deploy": "gulp deploy",
    "test": "gulp lint",
    "changelog": "git log --pretty=format:'* %s [View Commit](https://code.ipreo.com/Ipreo/cupcake/commit/%H)' > change.md",
    "changelog-2": "git log $(git describe --tags --abbrev=0)..HEAD --pretty=format:'* %s [View Commit](https://code.ipreo.com/Ipreo/cupcake/commit/%H)' | cut -d' ' -f 2- > change2.md",
    "list-selectors": "list-selectors ./dist/bd.css -p -i classes stdout > reports/class.json",
    "visual": "npm run reference && npm run compare",
    "reference": "backstop reference",
    "compare": "backstop test"
  },
  "lint-staged": {
    "*.scss": "stylelint --syntax=scss"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "backstopjs": "^3.0.32",
    "browser-sync": "^2.18.13",
    "colors": "^1.1.2",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.9.0",
    "gulp-downloader": "^1.0.4",
    "gulp-findreplace": "^1.5.0",
    "gulp-flatten": "^0.3.1",
    "gulp-gh-pages": "^0.5.4",
    "gulp-group-css-media-queries": "^1.2.0",
    "gulp-hb": "^6.0.2",
    "gulp-notify": "^3.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^7.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-style-extract": "^1.0.2",
    "gulp-stylelint": "^5.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "list-selectors": "^2.0.0",
    "octophant": "^1.0.1",
    "run-sequence": "^2.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-declaration-use-variable": "^1.6.0",
    "stylelint-scss": "^2.1.0",
    "surge": "^0.19.0",
    "vinyl-paths": "^2.1.0",
    "source-sans-pro": "^2.0.10"
  }
}
