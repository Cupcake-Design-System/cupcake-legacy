{
  "name": "@ipreo/cupcake",
  "version": "0.1.8",
  "description": "Ipreo CSS framework.",
  "main": "dist/default.css",
  "repository": {
    "type": "git",
    "url": "https://code.ipreo.com/Ipreo/cupcake"
  },
  "scripts": {
    "start": "gulp",
    "precommit": "lint-staged",
    "build": "gulp build",
    "deploy": "gulp deploy",
    "test": "gulp lint",
    "changelog": "git log --pretty=format:'* %s [View Commit](https://code.ipreo.com/Ipreo/cupcake/commit/%H)' > change.md",
    "list-selectors": "list-selectors ./dist/bd.css -p -i classes stdout > reports/class.json",
    "backstop": "npm run build & npm run reference & npm run compare",
    "reference": "backstop reference --configPath=backstop.config.js",
    "compare": "backstop test --configPath=backstop.config.js"
  },
  "lint-staged": {
    "*.scss": "stylelint --syntax=scss"
  },
  "devDependencies": {
    "autoprefixer": "^7.0.1",
    "browser-sync": "^2.0.0-rc4",
    "colors": "^1.1.2",
    "del": "^3.0.0",
    "gulp": "^3.9.0",
    "gulp-clean-css": "^3.4.2",
    "gulp-conventional-changelog": "^1.1.3",
    "gulp-downloader": "^1.0.4",
    "gulp-findreplace": "^1.5.0",
    "gulp-flatten": "^0.3.1",
    "gulp-gh-pages": "^0.5.4",
    "gulp-hb": "^6.0.2",
    "gulp-notify": "^3.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^7.0.0",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.4.0",
    "gulp-style-extract": "^1.0.2",
    "gulp-stylelint": "^3.7.0",
    "husky": "^0.14.1",
    "lint-staged": "^4.0.0",
    "list-selectors": "^2.0.0",
    "octophant": "^1.0.1",
    "run-sequence": "^1.2.2",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-declaration-use-variable": "^1.6.0",
    "stylelint-scss": "^1.4.1",
    "vinyl-paths": "^2.0.0"
  }
}
