{
  "name": "@ipreo/cupcake",
  "version": "1.4.13-beta.7",
  "description": "Ipreo CSS framework.",
  "main": "dist/default.css",
  "repository": {
    "type": "git",
    "url": "https://code.ipreo.com/Ipreo/cupcake"
  },
  "scripts": {
    "start": "gulp",
    "deploy:dist": "surge --project ./dist --domain ipreo-cupcake.surge.sh",
    "deploy:tests": "surge --project ./test --domain ipreo-cupcake-test.surge.sh",
    "deploy:surge": "npm run deploy:dist && npm run deploy:tests",
    "precommit": "lint-staged",
    "build": "gulp build",
    "build:prod": "gulp build && npm run copy-cdn-artifacts",
    "deploy": "gulp deploy",
    "test": "npm run test:default && npm run test:bd",
    "test:default": "karma start --default",
    "test:bd": "karma start --bd",
    "copy-cdn-artifacts": "node ./scripts/copy-cdn-artifacts.js",
    "publish": "node ./scripts/publish.js",
    "release": "standard-version",
    "changelog": "auto-changelog -p && git add CHANGELOG.md",
    "list-selectors-BD": "list-selectors ./dist/bd.css -p -i classes stdout > bd-class.json",
    "list-selectors": "list-selectors ./dist/default.css -p -i classes stdout > cupcake-class.json",
    "visual": "npm run reference && npm run compare",
    "reference": "backstop reference",
    "compare": "backstop test"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "changelog-template.hbs",
    "commitLimit": false,
    "package": true
  },
  "lint-staged": {
    "*.scss": "stylelint --syntax=scss"
  },
  "devDependencies": {
    "@ipreo/cupcake-stylelint-config": "1.1.0",
    "auto-changelog": "^1.8.0",
    "autoprefixer": "^7.2.1",
    "backstopjs": "^3.0.32",
    "browser-sync": "^2.18.13",
    "chai": "^4.1.2",
    "chai-jquery": "^2.0.0",
    "colors": "^1.1.2",
    "del": "^3.0.0",
    "event-stream": "^4.0.1",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.9.0",
    "gulp-clone": "^2.0.1",
    "gulp-downloader": "^1.0.4",
    "gulp-findreplace": "^1.5.0",
    "gulp-flatten": "^0.3.1",
    "gulp-footer": "^1.0.5",
    "gulp-gh-pages": "^0.5.4",
    "gulp-group-css-media-queries": "^1.2.0",
    "gulp-hb": "^6.0.2",
    "gulp-header": "^1.8.9",
    "gulp-notify": "^3.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^7.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-style-extract": "^1.0.2",
    "gulp-stylelint": "^5.0.0",
    "gulp-transform": "^3.0.5",
    "husky": "^0.14.3",
    "jquery": "^3.3.1",
    "karma": "^2.0.2",
    "karma-browserify": "^5.2.0",
    "karma-chai": "^0.1.0",
    "karma-chai-jquery": "^1.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jquery": "^0.2.2",
    "karma-jquery-chai": "^0.1.3",
    "karma-mocha": "^0.1.4",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "lint-staged": "^6.0.0",
    "list-selectors": "^2.0.0",
    "mocha": "^5.1.1",
    "ncp": "^2.0.0",
    "npm": "^6.9.0",
    "octophant": "^1.0.1",
    "release-it": "^7.6.1",
    "run-sequence": "^2.2.0",
    "sass-flatten": "^0.1.1",
    "source-sans-pro": "^2.0.10",
    "standard-version": "^6.0.1",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-declaration-use-variable": "^1.6.1",
    "stylelint-scss": "^3.1.3",
    "surge": "^0.19.0",
    "vinyl-paths": "^2.1.0"
  }
}
