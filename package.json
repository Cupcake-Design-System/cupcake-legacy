{
  "name": "@cupcake-ds/cupcake-legacy",
  "version": "1.4.20",
  "description": "Design System framework.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cupcake-Design-System/Cupcake-legacy/"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "gulp",
    "deploy:dist": "surge --project ./dist --domain ipreo-cupcake.surge.sh",
    "deploy:tests": "surge --project ./test --domain ipreo-cupcake-test.surge.sh",
    "deploy:surge": "npm run deploy:dist && npm run deploy:tests",
    "precommit": "lint-staged",
    "build": "gulp build",
    "build:prod": "gulp build",
    "deploy": "gulp deploy",
    "test": "npm run test:default && npm run test:bd && npm run test:ihs-bd",
    "test:default": "karma start --default",
    "test:bd": "karma start --bd",
    "test:ihs-bd": "karma start --ihs-bd",
    "release": "standard-version",
    "changelog": "auto-changelog -p && git add CHANGELOG.md",
    "list-selectors-BD": "list-selectors ./dist/bd.css -p -i classes stdout > bd-class.json",
    "list-selectors": "list-selectors ./dist/default.css -p -i classes stdout > cupcake-class.json",
    "visual": "npm run reference && npm run compare",
    "reference": "backstop reference",
    "compare": "backstop test"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "changelog-template.hbs",
    "commitLimit": false,
    "package": true
  },
  "lint-staged": {
    "*.scss": "stylelint --syntax=scss"
  },
  "devDependencies": {
    "auto-changelog": "^1.16.1",
    "autoprefixer": "^9.6.5",
    "browser-sync": "^2.26.7",
    "chai": "^4.2.0",
    "chai-jquery": "^2.1.0",
    "colors": "^1.4.0",
    "del": "^5.1.0",
    "event-stream": "^4.0.1",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^4.2.0",
    "gulp-clone": "^2.0.1",
    "gulp-downloader": "^1.0.4",
    "gulp-findreplace": "^1.5.0",
    "gulp-flatten": "^0.4.0",
    "gulp-footer": "^2.0.2",
    "gulp-gh-pages": "^0.5.4",
    "gulp-group-css-media-queries": "^1.2.2",
    "gulp-hb": "^8.0.0",
    "gulp-header": "^2.0.9",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-style-extract": "^1.0.2",
    "gulp-stylelint": "^10.0.0",
    "gulp-transform": "^3.0.5",
    "husky": "^3.0.9",
    "jquery": "^3.4.1",
    "karma": "^6.3.14",
    "karma-browserify": "^6.1.0",
    "karma-chai": "^0.1.0",
    "karma-chai-jquery": "^1.0.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^1.2.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jquery": "^0.2.4",
    "karma-jquery-chai": "^0.1.3",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "lint-staged": "^9.4.2",
    "list-selectors": "^2.0.0",
    "mocha": "^6.2.1",
    "natives": "^1.1.6",
    "ncp": "^2.0.0",
    "npm": "^6.12.0",
    "octophant": "^1.0.1",
    "release-it": "^12.4.3",
    "run-sequence": "^2.2.1",
    "sass-flatten": "^0.1.1",
    "source-sans-pro": "^3.6.0",
    "standard-version": "^7.0.0",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-declaration-use-variable": "^1.7.0",
    "stylelint-scss": "^3.11.1",
    "vinyl-paths": "^3.0.0"
  }
}
